              <img class="lazyload blur-up js-prd-img"
                         data-src="{{ 'openpay-new.png' | asset_img_url: 'x40' }}"
                         data-srcset="{{ 'openpay-new.png' | asset_img_url: 'x40' }} 1x, {{ 'openpay-new.png' | asset_img_url: 'x40' , scale: 2 }} 2x"
                         data-sizes="auto">
          </div>
          <div class="shop-feature-text">
            <div class="text1">{{ section.settings.caption_2_1_text }}</div>
            <div class="text2">{{ section.settings.caption_2_2_text }}</div>
          </div>
          <span class="shop-feature-bg"></span>
        </a>
      </div>
      <div class="col-md-4">
        <a {% if section.settings.caption_3_link != blank %}href="{{ section.settings.caption_3_link }}"{% endif %} class="shop-feature">
          {% comment %} <div class="shop-feature-icon"><i class="{{ section.settings.caption_3_icon }}"></i></div> {% endcomment %}
          <div class="shop-feature-icon">
              <img class="lazyload blur-up js-prd-img"
                         data-src="{{ 'laybuy-new2.png' | asset_img_url: 'x40'}}"
                         data-srcset="{{ 'laybuy-new2.png' | asset_img_url: 'x40'}} 1x, {{ 'laybuy-new2.png' | asset_img_url: 'x40' , scale: 2 }} 2x"
                         data-sizes="auto">
          </div>
          <div class="shop-feature-text">
            <div class="text1">{{ section.settings.caption_3_1_text }}</div>
            <div class="text2">{{ section.settings.caption_3_2_text }}</div>
          </div>
          <span class="shop-feature-bg"></span>
        </a>
      </div> 
      <div class="col-md-4">
        <a {% if section.settings.caption_4_link != blank %}href="{{ section.settings.caption_4_link }}"{% endif %} class="shop-feature">
          {% comment %} <div class="shop-feature-icon"><i class="{{ section.settings.caption_4_icon }}"></i></div> {% endcomment %}
          <div class="shop-feature-icon">
	  <img class="lazyload blur-up js-prd-img"
                         data-src="{{ 'klarna.svg' | asset_url }}"
                         data-srcset="{{ 'klarna.svg' | asset_url }} 1x"
                         data-sizes="auto">
          </div>
          <div class="shop-feature-text">
            <div class="text1">{{ section.settings.caption_4_1_text }}</div>
            <div class="text2">{{ section.settings.caption_4_2_text }}</div>
          </div>
          <span class="shop-feature-bg"></span>
        </a>
      </div>
	  <div class="col-md-4">
        <a {% if section.settings.caption_5_link != blank %}href="{{ section.settings.caption_5_link }}"{% endif %} class="shop-feature">
          {% comment %} <div class="shop-feature-icon"><i class="{{ section.settings.caption_5_icon }}"></i></div> {% endcomment %}
          <div class="shop-feature-icon">
           <img class="lazyload blur-up js-prd-img"
                         data-src="{{ 'amazon-Pay.png' | asset_img_url: 'x40' }}"
                         data-srcset="{{ 'amazon-Pay.png' | asset_img_url: 'x40' }} 1x, {{ 'amazon-Pay.png' | asset_img_url: 'x40' , scale: 2 }} 2x"
                         data-sizes="auto">
          </div>
          <div class="shop-feature-text">
            <div class="text1">{{ section.settings.caption_5_1_text }}</div>
            <div class="text2">{{ section.settings.caption_5_2_text }}</div>
          </div>
          <span class="shop-feature-bg"></span>
        </a>
      </div>
      <div class="col-md-4">
        <a {% if section.settings.caption_6_link != blank %}href="{{ section.settings.caption_6_link }}"{% endif %} class="shop-feature">
          {% comment %} <div class="shop-feature-icon"><i class="{{ section.settings.caption_6_icon }}"></i></div> {% endcomment %}
          <div class="shop-feature-icon">
           <img class="lazyload blur-up js-prd-img"
