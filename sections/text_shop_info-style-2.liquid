                         data-srcset="{{ 'colour.png' | asset_img_url: 'x40' }} 1x, {{ 'colour.png' | asset_img_url: 'x40' , scale: 2 }} 2x"
                         data-sizes="auto">
          </div>
          <div class="shop-feature-text">
            <div class="text1">{{ section.settings.caption_6_1_text }}</div>
            <div class="text2">{{ section.settings.caption_6_2_text }}</div>
          </div>
          <span class="shop-feature-bg"></span>
        </a>
      </div>
    </div>
  </div>
</div>
{% endunless %}
