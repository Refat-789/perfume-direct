{% unless settings.slides_mode %}
<div class="holder  boxed  {{ section.settings.outer_margin }} {% if section.settings.show_under_header %}show_under_header{% endif %}">
  <div class="container holder-bg-12" {% if section.settings.bg != blank %}style="background-color: {{ section.settings.bg }}"{% endif %}>
    <div class="row shop-features-style3">
      <div class="col-md-4">
        <a {% if section.settings.caption_1_link != blank %}href="{{ section.settings.caption_1_link }}"{% endif %} class="shop-feature">
          {% comment %} <div class="shop-feature-icon"><i class="{{ section.settings.caption_1_icon }}"></i></div> {% endcomment %}
          <div class="shop-feature-icon">
           <img class="lazyload blur-up js-prd-img"
                         data-src="{{ 'clearpay-new.png' | asset_img_url: 'x40' }}"
                         data-srcset="{{ 'clearpay-new.png' | asset_img_url: 'x40' }} 1x, {{ 'clearpay-new.png' | asset_img_url: 'x40' , scale: 2 }} 2x"
                         data-sizes="auto">
          </div>
          <div class="shop-feature-text">
            <div class="text1">{{ section.settings.caption_1_1_text }}</div>
            <div class="text2">{{ section.settings.caption_1_2_text }}</div>
          </div>
          <span class="shop-feature-bg"></span>
        </a>
      </div>
      <div class="col-md-4">
        <a {% if section.settings.caption_2_link != blank %}href="{{ section.settings.caption_2_link }}"{% endif %} class="shop-feature">
          {% comment %} <div class="shop-feature-icon"><i class="{{ section.settings.caption_2_icon }}"></i></div> {% endcomment %}
